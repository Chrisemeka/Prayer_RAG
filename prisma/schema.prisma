generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./rag_data.db"
}

model Verses {
  id                 String    @id
  reference          String 
  text               String
  book_name          String
  book_number        Int
  chapter            Int
  verse              Int
  text_length        Int
  embedding_id       String
  created_at         DateTime  @default(now())
  index              Int

  @@map("verses")
}

model Therapy_Manual {
  id                 String    @id
  title              String
  content            String
  created_at         DateTime  @default(now())

  @@map("therapy_manual")
}